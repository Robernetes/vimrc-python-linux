import random


minus = 'abcdefghijklmnopqrstuvwxyz'
mayus = 'ABCDEFGHIJKLMNOPQRSTUVWXYZ'
number = '0123456789'
symbol = '{}{}()*:/,._-@#'

todo = minus + mayus + number + symbol

cant = 16

password = "".join(random.sample(todo,cant))

